{
  "name": "unocss-preset-antd",
  "version": "0.0.1-beta.0",
  "packageManager": "pnpm@8.3.1",
  "description": "unocss + antd",
  "author": "Dunqing <dengqing0821@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/Dunqing/unocss-preset-antd#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dunqing/unocss-preset-antd.git",
    "directory": "packages/preset-theme"
  },
  "bugs": {
    "url": "https://github.com/Dunqing/unocss-preset-antd/issues"
  },
  "keywords": [
    "unocss",
    "unocss-preset",
    "unocss-theme",
    "antd",
    "antd-theme"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "*.css"
  ],
  "scripts": {
    "build": "unbuild",
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "release": "bumpp package.json packages/**/package.json",
    "stub": "unbuild --stub",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "unocss": "^0.51.0"
  },
  "dependencies": {
    "@ant-design/colors": "^7.0.0",
    "unocss": "^0.51.6",
    "unocss-preset-theme": "^0.6.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.5",
    "@types/node": "^18.16.0",
    "bumpp": "^9.1.0",
    "eslint": "^8.39.0",
    "unbuild": "^1.2.1",
    "vitest": "^0.30.1"
  }
}
